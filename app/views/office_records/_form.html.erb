<%= form_for(@office_record, :url => url, :method => method) do |f| %>
  <% if @office_record.errors.any? %>
    <div id="error_explanation">
      <h2>Imposible registrar informaci√≥n</h2>
      <ul>
        <% @office_record.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  </br>
  <div class="field">
    <%= f.label :office_id %><br />
    <%= f.number_field :office_id %>
  </div>
