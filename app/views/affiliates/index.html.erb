<h1>Listing affiliates</h1>

<table>
  <tr>
    <th>Public target</th>
    <th>Program</th>
    <th>Period time</th>
    <th>Period number</th>
    <th>Institution</th>
    <th>Person</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @affiliates.each do |affiliate| %>
  <tr>
    <td><%= affiliate.public_target_id %></td>
    <td><%= affiliate.program_id %></td>
    <td><%= affiliate.period_time_id %></td>
    <td><%= affiliate.period_number %></td>
    <td><%= affiliate.institution_id %></td>
    <td><%= affiliate.person_id %></td>
    <td><%= link_to 'Show', affiliate %></td>
    <td><%= link_to 'Edit', edit_affiliate_path(affiliate) %></td>
    <td><%= link_to 'Destroy', affiliate, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Affiliate', new_affiliate_path %>
