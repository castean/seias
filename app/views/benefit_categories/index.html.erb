<h1>Listing benefit_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @benefit_categories.each do |benefit_category| %>
  <tr>
    <td><%= benefit_category.name %></td>
    <td><%= link_to 'Show', benefit_category %></td>
    <td><%= link_to 'Edit', edit_benefit_category_path(benefit_category) %></td>
    <td><%= link_to 'Destroy', benefit_category, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

