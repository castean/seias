<h1>Listing priority_program_strategies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Hierarchy</th>
    <th>Priority program goal</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @priority_program_strategies.each do |priority_program_strategy| %>
  <tr>
    <td><%= priority_program_strategy.name %></td>
    <td><%= priority_program_strategy.description %></td>
    <td><%= priority_program_strategy.hierarchy %></td>
    <td><%= priority_program_strategy.priority_program_goal_id %></td>
    <td><%= link_to 'Show', priority_program_strategy %></td>
    <td><%= link_to 'Edit', edit_priority_program_strategy_path(priority_program_strategy) %></td>
    <td><%= link_to 'Destroy', priority_program_strategy, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

