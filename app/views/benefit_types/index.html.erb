<h1>Listing benefit_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Benefit category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @benefit_types.each do |benefit_type| %>
  <tr>
    <td><%= benefit_type.name %></td>
    <td><%= benefit_type.benefit_category_id %></td>
    <td><%= link_to 'Show', benefit_type %></td>
    <td><%= link_to 'Edit', edit_benefit_type_path(benefit_type) %></td>
    <td><%= link_to 'Destroy', benefit_type, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Benefit type', new_benefit_type_path %>
