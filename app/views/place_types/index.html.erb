<h1>Listing place_types</h1>

<table>
  <tr>
    <th>Nick</th>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @place_types.each do |place_type| %>
  <tr>
    <td><%= place_type.nick %></td>
    <td><%= place_type.name %></td>
    <td><%= place_type.description %></td>
    <td><%= link_to 'Show', place_type %></td>
    <td><%= link_to 'Edit', edit_place_type_path(place_type) %></td>
    <td><%= link_to 'Destroy', place_type, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

