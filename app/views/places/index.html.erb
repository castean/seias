<h1>Listing places</h1>

<table>
  <tr>
    <th>Place type</th>
    <th>Name</th>
    <th>Address</th>
    <th>Town</th>
    <th>Info</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Gmaps</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.place_type_id %></td>
    <td><%= place.name %></td>
    <td><%= place.address %></td>
    <td><%= place.town_id %></td>
    <td><%= place.info %></td>
    <td><%= place.latitude %></td>
    <td><%= place.longitude %></td>
    <td><%= place.gmaps %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>
</br>
<% form_for :dump, :url=>importarlugares_path, :html => { :multipart => true } do |f| -%>
	<label for="dump_file">
        Select a CSV File :
    </label>
    <%= f.file_field :file -%>
   </br>
    <%= submit_tag 'Importar Lugares' -%>
<% end %>
</br>
<br />

